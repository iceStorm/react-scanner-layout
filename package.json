{
  "name": "react-scanner-layout",
  "version": "0.3.3",
  "type": "module",
  "license": "MIT",
  "main": "./index.js",
  "types": "./index.d.ts",
  "files": [
    "./*"
  ],
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./menu": {
      "import": "./menu.js",
      "require": "./menu.cjs"
    },
    "./style.css": {
      "import": "./style.css",
      "require": "./style.css"
    }
  },
  "homepage": "https://react-scanner-layout.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/iceStorm/react-scanner-layout"
  },
  "keywords": [
    "react",
    "react component",
    "webcam",
    "scanner layout",
    "react scanner"
  ],
  "scripts": {
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "dev": "vite --config vite.config.demo.ts",
    "preview": "npm run build:demo && vite preview --config vite.config.demo.ts",
    "build:demo": "tsc && vite build --config vite.config.demo.ts",
    "build:lib": "tsc && vite build --config vite.config.lib.ts",
    "publish:lib": "PUBLISH=true npm run build:lib && cd ./dist/lib && npm publish"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.14",
    "framer-motion": "^10.12.4",
    "immer": "^10.0.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.13",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@undecaf/zbar-wasm": "0.9.13",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "^4.0.0-beta.0",
    "autoprefixer": "^10.4.14",
    "clsx": "^1.2.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.23",
    "postcss-replace": "^2.0.1",
    "rc-switch": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-scanner-layout": "^0.3.3",
    "react-simple-toasts": "^3.6.0",
    "react-webcam": "^7.0.1",
    "rimraf": "^5.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.62.0",
    "simple-zustand-devtools": "^1.1.0",
    "tailwindcss": "^3.3.1",
    "type-fest": "^3.8.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.0",
    "vite-aliases": "^0.11.0",
    "vite-plugin-css-injected-by-js": "^3.1.0",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-static-copy": "^0.14.0",
    "vite-plugin-wasm": "^3.2.2",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
